{
    "name": "ingress-event-shards",
    "version": "1.0.0",
    "description": "Interactive map of shard data from Ingress events",
    "type": "module",
    "private": true,
    "main": "dist/main.bundle.js",
    "scripts": {
        "clean": "rm -rf data && rm -rf dist",
        "start": "python scripts/geocode.py && webpack serve --config webpack.dev.js",
        "build": "python scripts/geocode.py && webpack --config webpack.prod.js"
    },
    "devDependencies": {
        "css-loader": "^7.1.2",
        "html-webpack-plugin": "^5.6.4",
        "style-loader": "^4.0.0",
        "webpack": "^5.101.3",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-merge": "^6.0.1"
    },
    "dependencies": {
        "country-flag-emoji-polyfill": "^0.1.8",
        "github-fork-ribbon-css": "^0.2.3",
        "jquery": "^3.7.1",
        "leaflet": "^1.9.4",
        "leaflet-providers": "^2.0.0",
        "leaflet.motion": "github:neon-ninja/leaflet.motion"
    }
}
